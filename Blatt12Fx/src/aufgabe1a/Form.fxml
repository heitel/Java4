<VBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aufgabe1a.FormController">
   <children>
      <Label text="Push the Button!">
         <VBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="button" mnemonicParsing="false" onAction="#doClick" prefWidth="100.0" text="Test">
